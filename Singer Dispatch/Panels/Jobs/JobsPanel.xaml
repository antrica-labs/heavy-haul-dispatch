<UserControl x:Class="SingerDispatch.Panels.Jobs.JobsPanel"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/SingerDispatchDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <StackPanel>
        <TabControl Name="tabs">
            <TabItem Header="Job History" />
            <TabItem Header="Loads" />
            <TabItem Header="Commodities" />
            <TabItem Header="Dispatches" />
            <TabItem Header="Third Party Services" />
            <TabItem Header="Permits" />
        </TabControl>
    </StackPanel>
</UserControl>
