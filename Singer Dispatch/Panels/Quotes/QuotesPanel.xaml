<UserControl x:Class="SingerDispatch.Panels.Quotes.QuotesPanel"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Quote="clr-namespace:SingerDispatch.Panels.Quotes"
    IsEnabled="True">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/SingerDispatchDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <StackPanel>
        <Border Background="LightGoldenrodYellow" BorderBrush="LightGray" BorderThickness="1" Margin="0,0,0,5">
            <Label>Active Quote:</Label>
        </Border>
        
        <TabControl Name="tabs">
            <TabItem Header="Quote History">
                <Quote:QuoteHistoryControl />
            </TabItem>
            <TabItem Header="Move Commodity" />            
            <TabItem Header="Supplements" />            
            <TabItem Header="Storage" />
            <TabItem Header="Conditions" />
        </TabControl>
    </StackPanel>
    
</UserControl>
