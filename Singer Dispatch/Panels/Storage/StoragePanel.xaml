<UserControl x:Class="SingerDispatch.Panels.Storage.StoragePanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:Storage="clr-namespace:SingerDispatch.Panels.Storage"
             mc:Ignorable="d">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/SingerDispatchDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <TabControl Name="Tabs">
            <TabItem Header="Storage List">
                <Storage:StorageListControl />
            </TabItem>
            <TabItem Header="Billing"></TabItem>
        </TabControl>
    </Grid>
</UserControl>
