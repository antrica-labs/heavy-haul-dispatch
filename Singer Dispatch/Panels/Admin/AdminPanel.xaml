<UserControl x:Class="SingerDispatch.Panels.Admin.AdminPanel"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Admin="clr-namespace:SingerDispatch.Panels.Admin">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/SingerDispatchDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <TabControl Name="Tabs">
            <TabItem Header="Employees">
                <Admin:EmployeesControl Width="Auto" />    
            </TabItem>
            <TabItem Header="Equipment">
                <Admin:EquipmentControl Width="Auto" />
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>
